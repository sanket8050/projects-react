{
  "name": "statusly-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev -p %PORT%\" \"node index.js\"",
    "build": "next build",
    "start": "concurrently \"next start -p %PORT%\" \"node index.js\"",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.47",
    "prisma": "^5.18.0",
    "tailwindcss": "^3.4.13"
  }
}




